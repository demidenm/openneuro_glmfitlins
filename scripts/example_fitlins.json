{
  "Name": "Flanker",
  "BIDSModelVersion": "1.0.0",
  "Input": {"subject": ["01", "02", "03"], "task": ["flanker"]},
  "Nodes": [
    {
      "Level": "Run",
      "Name": "run_level",
      "GroupBy": ["run", "subject"],
      "Transformations": {
        "Transformer": "pybids-transforms-v1",
        "Instructions": [
          {
            "Name": "Factor",
            "Input": ["trial_type"]
          },
          {
            "Name": "Rename",
            "Input": [
              "trial_type.incongruent_correct",
              "trial_type.congruent_correct"
              ],
            "Output": [
              "incongruent_correct",
              "congruent_correct"
              ]
            },
      {
        "Name": "Convolve",
        "Model": "spm",
        "Input": [
          "incongruent_correct",
          "congruent_correct"
        ]
      }
    ]
  },
      "Model": {"X": [1, "incongruent_correct","congruent_correct", "trans_*", "rot*", "cosine*"], "Type": "glm"},
      "Contrasts": [
        {
          "Name": "incong_vs_congruent",
          "ConditionList": ["incongruent_correct", "congruent_correct"],
          "Weights": [1, -1],
          "Test": "t"
        }
      ]
    },
    {
      "Level": "Subject",
      "Name": "subject_level",
      "GroupBy": ["subject", "contrast"],
      "Model": {"X": [1], "Type": "glm"},
      "DummyContrasts": {"Test": "t"}
    }
  ]
}