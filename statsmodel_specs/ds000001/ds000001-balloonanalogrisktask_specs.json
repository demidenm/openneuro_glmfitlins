{
  "Name": "ds000001",
  "BIDSModelVersion": "1.0.0",
  "Input": {
    "subject": [
      "01",
      "02",
      "03",
      "04",
      "05",
      "06",
      "07",
      "08",
      "09",
      "10",
      "11",
      "12",
      "13",
      "14",
      "15",
      "16"
    ],
    "task": [
      "balloonanalogrisktask"
    ]
  },
  "Nodes": [
    {
      "Level": "Run",
      "Name": "run_level",
      "GroupBy": [
        "run",
        "subject"
      ],
      "Transformations": {
        "Transformer": "pybids-transforms-v1",
        "Instructions": [
          {
            "Name": "Copy",
            "Input": ["trial_type"],
            "Output": ["rt_reg"]
          },
          {
            "Name": "Replace",
            "Input": ["rt_reg"],
            "Replace": {
                "cash_demean": "rt",
                "control_pumps_demean": "rt",
                "explode_demean": "rt",
                "pumps_demean": "rt"
                },
            "Attribute": "value"
          },
          {
            "Name": "Factor",
            "Input": [
              "trial_type", "rt_reg"
            ]
          },
          {
            "Name": "Assign",
            "Input": ["response_time"],
            "Target": ["rt_reg.rt"],
            "TargetAttr": "duration"
          },
          {
            "Name": "Convolve",
            "Model": "spm",
            "Input": [
              "trial_type.*",
              "cash_demean",
              "control_pumps_demean",
              "explode_demean",
              "pumps_demean",
              "rt_reg.rt"
            ]
          }
        ]
      },
      "Model": {
        "X": [
          1,
          "trial_type.*",
          "cash_demean",
          "control_pumps_demean",
          "explode_demean",
          "pumps_demean",
          "rt_reg.rt",
          "trans_*",
          "rot_*",
          "cosine*"
        ],
        "Type": "glm"
      },
      "Contrasts": [
          {
              "Name": "pumpsvcontrolpumps",
              "ConditionList": [
                  "trial_type.pumps_demean",
                  "trial_type.control_pumps_demean"
              ],
              "Weights": [
                  1,
                  -1
              ],
              "Test": "t"
          },
          {
              "Name": "explodevcash",
              "ConditionList": [
                  "trial_type.explode_demean",
                  "trial_type.cash_demean"
              ],
              "Weights": [
                  1,
                  -1
              ],
              "Test": "t"
          },
          {
              "Name": "explodevcontrol",
              "ConditionList": [
                  "trial_type.explode_demean",
                  "trial_type.control_pumps_demean"
              ],
              "Weights": [
                  1,
                  -1
              ],
              "Test": "t"
          },
          {
              "Name": "pumpscashvcontrol",
              "ConditionList": [
                  "trial_type.pumps_demean",
                  "trial_type.cash_demean",
                  "trial_type.control_pumps_demean"
              ],
              "Weights": [
                  0.5,
                  0.5,
                  -1
              ],
              "Test": "t"
          },
          {
              "Name": "pumpsexplodevcontrol",
              "ConditionList": [
                  "trial_type.pumps_demean",
                  "trial_type.explode_demean",
                  "trial_type.control_pumps_demean"
              ],
              "Weights": [
                  0.5,
                  0.5,
                  -1
              ],
              "Test": "t"
          },
          {
              "Name": "allpumps",
              "ConditionList": [
                  "trial_type.pumps_demean",
                  "trial_type.cash_demean",
                  "trial_type.explode_demean"
              ],
              "Weights": [
                  0.33,
                  0.33,
                  0.33
              ],
              "Test": "t"
          },
          {
              "Name": "pumpsdemean",
              "ConditionList": [
                  "trial_type.pumps_demean"
              ],
              "Weights": [
                  1
              ],
              "Test": "t"
          },
          {
              "Name": "pumpspara",
              "ConditionList": [
                  "pumps_demean"
              ],
              "Weights": [
                  1
              ],
              "Test": "t"
          },
          {
              "Name": "cashpara",
              "ConditionList": [
                  "cash_demean"
              ],
              "Weights": [
                  1
              ],
              "Test": "t"
          },
          {
              "Name": "explodepara",
              "ConditionList": [
                  "explode_demean"
              ],
              "Weights": [
                  1
              ],
              "Test": "t"
          },
          {
              "Name": "controlpara",
              "ConditionList": [
                  "control_pumps_demean"
              ],
              "Weights": [
                  1
              ],
              "Test": "t"
          },
          {
              "Name": "rt",
              "ConditionList": [
                  "rt_reg.rt"
              ],
              "Weights": [
                  1
              ],
              "Test": "t"
          }
      ]
    },
    {
      "Level": "Subject",
      "Name": "subject_level",
      "GroupBy": [
        "subject",
        "contrast"
      ],
      "Model": {
        "X": [
          1
        ],
        "Type": "glm"
      },
      "DummyContrasts": {
        "Test": "t"
      }
    },
    {
      "Level": "Dataset",
      "Name": "data_level",
      "GroupBy": [
        "contrast"
      ],
      "Model": {
        "X": [
          1
        ],
        "Type": "glm"
      },
      "DummyContrasts": {
        "Test": "t"
      }
    }
  ]
}